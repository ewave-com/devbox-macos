# Roots of the synchronization
root = socket://127.0.0.1:unison_port/WEBSITE_DOCUMENT_ROOT
root = ./projects/project_folder/public_html

# Paths to synchronize 
path = ./
path = ./

# Prevent permissions updating
perms=0
dontchmod=true

# Params
fastercheckUNSAFE = true
ignorelocks = true
numericids = false
log = true
batch = true
fastcheck = true
maxthreads = 100
confirmbigdel = false
ignore = Path .git
ignore = Path .idea
ignore = Path .gitignore
ignore = Path .gitattributes
ignore = Path var/cache
ignore = Path var/composer_home
ignore = Path var/log
ignore = Path var/view_preprocessed
ignore = Path var/page_cache
ignore = Path var/session
ignore = Path var/tmp
ignore = Path pub/static
ignore = Path .magento
ignore = Path .npm
ignore = Path var/.setup_cronjob_status
ignore = Path var/.update_cronjob_status
ignore = Path */update/vendor/composer/composer/tests/Composer/Test/Autoload/Fixtures/classmap/LargeClass.php
prefer = socket://127.0.0.1:unison_port/WEBSITE_DOCUMENT_ROOT